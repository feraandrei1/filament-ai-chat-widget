{
    "name": "ferarandrei1/filament-ai-chat-widget",
    "description": "A beautiful AI chat widget plugin for Filament v3 with OpenAI integration",
    "keywords": [
        "filament",
        "laravel",
        "livewire",
        "openai",
        "ai",
        "chat",
        "chatbot",
        "assistant",
        "filament-plugin"
    ],
    "homepage": "https://github.com/ferarandrei1/filament-ai-chat-widget",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Feraru Andrei",
            "email": "feraruandrei@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "filament/filament": "^3.0",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "livewire/livewire": "^3.0",
        "openai-php/laravel": "^0.10|^0.11|^0.12|^0.13|^0.14|^0.15|^0.16|^0.17",
        "laravel/mcp": "^0.1"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Feraandrei1\\FilamentAiChatWidget\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Feraandrei1\\FilamentAiChatWidget\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Feraandrei1\\FilamentAiChatWidget\\FilamentAiChatServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
